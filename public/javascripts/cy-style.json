[
  {
    "selector": "node",
    "style": {
      "background-color": "#74bef7",
      "width": 30,
      "height": 30,
      "label": "data(id)",
      "color": "#fff"
    }
  },

  {
    "selector": "edge",
    "style": {
      "display": "none"
    }
  },
  {
    "selector": "node:selected",
    "style": {
      "background-color": "#FFFF99",
      "label": "data(id)"
    }
  },

  {
    "selector": "edge:selected",
    "style": {
      "display": "element"
    }
  },

  {
    "selector": "edge.hovered",
    "style": {
      "display": "element",
      "curve-style": "bezier",
      "width": 3,
      "line-color": "#fcf4f4",
      "target-arrow-color": "#fcf4f4",
      "target-arrow-shape": "triangle"
    }
  }
]