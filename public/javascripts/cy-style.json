[
  {
    "selector": "node",
    "style": {
      "background-color": "#700000",
      "label": "data(id)",
      "width": "label",
      "height": "label",
      "text-halign": "center",
      "text-valign": "center",
      "font-size": 20,
      "text-outline-opacity": 1,
      "text-outline-width": 1,
      "padding": 5,
      "color": "#ffffff",
      "shape": "roundrectangle",
      "border-opacity": 1,
      "border-color": "#ffffff",
      "border-width": 2,
      "border-style": "solid"
    }
  },

  {
    "selector": "node.available",
    "style": {
      "background-color": "#00A7E1"
    }
  },

  {
    "selector": "node.enabled",
    "style": {
      "background-color": "#53ba4c"
    }
  },

  {
    "selector": "edge",
    "style": {
      "display": "none",
      "curve-style": "bezier",
      "width": 3,
      "line-color": "#ffffff",
      "target-arrow-color": "#ffffff",
      "target-arrow-shape": "triangle"
    }
  },

  {
    "selector": "edge.enabled",
    "style": {
      "line-color": "#53ba4c",
      "target-arrow-color": "#53ba4c"
    }
  },

  {
    "selector": "edge.hovered",
    "style": {
      "display": "element"
    }
  },

  {
    "selector": "edge.optional",
    "style": {
      "line-style": "dashed"
    }
  }
]